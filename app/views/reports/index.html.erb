<div class="container">
   <h2>レポート一覧</h2>
   <% @reports.each do |report| %>
      <%= report.name %>
      <%= report.item %>
      <%= report.content %>
      <%= link_to "Show", report_path(report) %>
      <%= link_to "Edit", edit_report_path(report) %>
    <% end %>
    
    <%= link_to "New", new_report_path %>
</div>
